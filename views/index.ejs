    <div class="container">
        <form action="/contact" method="get">
            <input type="search" name="nama">
            <button type="submit">filter</button>
        </form>
        <% data.forEach((item)=>{ %>
            <dl class="max-w-md text-gray-900 divide-y divide-gray-200 dark:text-white dark:divide-gray-700">
                <div class="flex flex-col pb-3">
                    <dt class="mb-1 text-gray-500 md:text-lg dark:text-gray-400">Nama</dt>
                    <dd class="text-lg font-semibold">
                        <%= item.name %>
                    </dd>
                </div>
                <div class="flex flex-col py-3">
                    <dt class="mb-1 text-gray-500 md:text-lg dark:text-gray-400">Phone</dt>
                    <dd class="text-lg font-semibold">
                        <%= item.number %>
                    </dd>
                </div>


                <% item.group.forEach((item_group)=> { %>
                    <div class="flex flex-col pt-3">
                        <dt class="mb-1 text-gray-500 md:text-lg dark:text-gray-400">Group</dt>
